                            $.get('/Search/Index', { searchTerm: searchTerm }, function (data) {
                                // Update the search results section with the returned HTML
                                $('#search-results').html(data);
                            });
                        };

                        // Function to initialize search
                        var initSearchFn = (ev) => {
                            ev.stopPropagation();
                            // Trim the input value
                            self.inputEl.value = self.inputEl.value.trim();

                            if (!classie.has(self.el, 'sb-search-open')) { // open it
                                ev.preventDefault();
                                self.open();
                            } else if (classie.has(self.el, 'sb-search-open') && /^\s*$/.test(self.inputEl.value)) { // close it
                                ev.preventDefault();
                                self.close();
                            }
                        };

                        // Function to open the search box
                        this.open = function () {
                            classie.add(self.el, 'sb-search-open');
                            // focus the input
                            if (!mobilecheck()) {
                                self.inputEl.focus();
                            }
                            // close the search input if body is clicked
                            var bodyFn = function (ev) {
                                self.close();
                                this.removeEventListener('click', bodyFn);
                                this.removeEventListener('touchstart', bodyFn);
                            };
                            document.addEventListener('click', bodyFn);
                            document.addEventListener('touchstart', bodyFn);
                        };

                        // Event listeners
                        this.el.addEventListener('click', initSearchFn);
                        this.el.addEventListener('touchstart', initSearchFn);
                        this.inputEl.addEventListener('click', function (ev) { ev.stopPropagation(); });
                        this.inputEl.addEventListener('touchstart', function (ev) { ev.stopPropagation(); });

                        // Add the form submission event listener
                        this.el.querySelector('form').addEventListener('submit', submitFormFn);
                    },

                    // Definition of the close method
                    close: function () {
                        this.inputEl.blur();
                        classie.remove(this.el, 'sb-search-open');
                    }

                    // ... (existing code)
                };



                // add to global namespace
                window.UISearch = UISearch;

            })(window);
        </script>
